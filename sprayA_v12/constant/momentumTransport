FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      momentumTransport;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  RAS;

RAS
{
    model           kEpsilon;

    turbulence      on;

    printCoeffs     on;

    kEpsilonCoeffs
    {
        Cmu		0.090;
        C1		1.550;
        C2		1.920;
        C3 	       -0.330;
        sigmak  	1.000;
        sigmaEps	1.300;//1.219;		
    }
}


