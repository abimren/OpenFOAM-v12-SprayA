FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver            ode;
}

chemistry       on;

cpuLoad         off;//true;

initialChemicalTimeStep 1e-06;

odeCoeffs
{
    solver          seulexDNS;
    absTol          1e-14;
    relTol          1e-4;
}

#include "reactions"

