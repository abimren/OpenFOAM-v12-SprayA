FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      CO2;
}
// ************************************************************************* //

dimensions          [];

internalField       uniform 0.09361;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    sideA
    {
        type            wedge;
    }
    sideB
    {
        type            wedge;
    }
    centerPlane
    {
        type            empty;
    }
}

